{
  "name": "es6-jspm-gulp-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for ES6+ apps using JSPM + Babel",
  "main": "dist/index.html",
  "author": {
    "name": "Alex Weber",
    "email": "alexweber15@gmail.com",
    "url": "http://alexweber.com.br"
  },
  "readmeFilename": "README.md",
  "license": {
    "type": "Unlicense",
    "url": "http://unlicense.org/"
  },
  "directories": {
    "doc": "docs",
    "test": "test",
    "baseURL": "src",
    "packages": "src/lib"
  },
  "scripts": {
    "test": "gulp test",
    "docs": "npm run sassdocs && npm run jsdocs",
    "jsdocs": "esdoc -c .esdoc.json",
    "sassdocs": "sassdoc src/scss"
  },
  "devDependencies": {
    "del": "^1.2.0",
    "esdoc": "^0.1.2",
    "eslint": "^0.22.1",
    "eslint-config-standard": "^3.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.5.2",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "^0.13.2",
    "gulp-minify-css": "^1.1.6",
    "gulp-minify-html": "^1.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.0.1",
    "gulp-scss-lint": "^0.2.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.5",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.0",
    "sassdoc": "^2.1.15",
    "systemjs-builder": "^0.11.3"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "doc": "docs",
      "packages": "src/lib",
      "test": "test"
    },
    "dependencies": {
      "pixi.js": "npm:pixi.js@^3.0.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.13",
      "babel-runtime": "npm:babel-runtime@^5.1.13",
      "core-js": "npm:core-js@^0.9.4"
    }
  }
}
